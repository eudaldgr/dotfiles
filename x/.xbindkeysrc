"amixer -q set Master toggle"
	XF86AudioMute

"amixer -q set Master 5%- unmute"
	XF86AudioLowerVolume

"amixer -q set Master 5%+ unmute"
	XF86AudioRaiseVolume

"xbacklight -dec 10"
	XF86MonBrightnessDown

"xbacklight -inc 10"
	XF86MonBrightnessUp

"sv hup wpa_supplicant"
	XF86WLAN

"scrot ~/lib/img/screenshots/%b%d::%H%M%S.png"
	Print

"pkill lemonbar | pkill sleep | bar"
	Control+Shift + space
